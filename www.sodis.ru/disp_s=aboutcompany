                                      <video alt="О компании" controls class="media-gallery__item-video">
                                          <source src="https://www.sodis.ru/content/video/sodis.mp4">
                                      </video>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </section>
                </div>
              </div>
